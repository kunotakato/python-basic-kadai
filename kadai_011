        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "#forループを使って1回出力\n",
        "for element in array:\n",
        "  print(element)\n",
        "\n",
        "# 続いてwhileループを使って１回出力\n",
        "i = 0\n",
        "while i < len(array):\n",
        "  print(array[i])\n",
        "  i += 1\n"
      ]
